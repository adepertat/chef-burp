###
# Generated by Chef for <%= node[:fqdn] %>
###

<% @settings.each do |key, value| -%>
<%  	next if value.nil? -%>
<%   	if value.kind_of? Array -%>
<% 			value.each do |v| -%>
<%= 			"#{key} = #{v}"%>
<%		   	end -%>
<%   	else -%>
<%= 		"#{key} = #{value}"%>
<%   	end -%>
<% end -%>
. <%= @config_dir %>/burp.conf.local
. <%= @config_dir %>/burp.conf.password
